.my_self {
    width: 100%;
    position: relative;
    overflow: hidden;
}

.ms-userinfo {
    display: flex;
    box-sizing: border-box;
    padding: 30rpx;
    width: 100%;
    min-height: 180rpx;
    border-bottom: 1px solid #eee;
    background-image: url('http://oibl5dyji.bkt.clouddn.com/myself_bg_2@2x.jpg');
    background-size: cover;
}

.ms-userinfo-item {
    align-items: center;
    font-size: 0;
    display: flex;
}

.ms-userinfo-avatar {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}

.ms-userinfo-text {
    flex: 1;
    font-size: 22rpx;
    color: #fff;
}

.ms-userinfo-base {
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    height: 100%;
    padding-left: 26rpx;
    font-size: 30rpx;
}

.ms-userinfo-nick {
    font-size: 32rpx;
    font-weight: bold;
    color: #fff;
    max-width: 280rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ms-userinfo-integral .ms-userinfo-integral-value {
    padding-left: 15rpx;
    font-size: 32rpx;
    font-weight: bold;
    color: #ff6b69;
}

.ms-userinfo-integral text {
    align-items: flex-end;
}

.ms-userinfo-integral navigator {
    display: inline-block;
}

.ms-userinfo-earn {
    box-sizing: border-box;
    margin-left: 25rpx;
    padding: 0 10rpx;
    border: 1px solid #fff;
    border-radius: 4rpx;
    font-size: 24rpx;
    color: #fff;
}

.ms-userinfo-used_space {
    padding-top: 10rpx;
}

.ms-userinfo-signin {
    flex: auto;
    display: flex;
    justify-content: flex-end;
}

.ms-userinfo-signin-btn {
    min-width: 208rpx;
    height: 75rpx;
    line-height: 75rpx;
    text-align: center;
    border-radius: 37rpx;
    background-color: #ff6b01;
}

.ms-userinfo-signin-btn_btn {
    padding: 0 20rpx;
    background-color: transparent;
    line-height: 1;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.ms-userinfo-signin-btn text {
    font-size: 26rpx;
    color: #333;
}

.ms-userinfo-signin-disabled {
    background-color: #999;
}

.ms-userinfo-signin-disabled text {
    color: #fff;
}

.ms-temp_tip {
    display: flex;
    box-sizing: border-box;
    align-items: center;
    padding: 18rpx 36rpx;
    background-color: #f1f1f1;
}

.ms-tt-image {
    display: flex;
    align-items: center;
    width: 45rpx;
}

.ms-tt-image image {
    width: 45rpx;
    height: 45rpx;
}

.ms-tt-text {
    flex: auto;
    padding-left: 26rpx;
    font-size: 22rpx;
    text-align: left;
    color: #666;
}

.ms-links {
    padding: 59rpx 50rpx 39rpx;
    display: flex;
    justify-content: space-around;
}

.ms-links-item {
}

.ms-links-item_btn {
    display: flex;
    justify-content: center;
    width: 100%;
    height: 100%;
    background-color: transparent;
    padding: 0;
    line-height: 1;
}

.ms-links-item-icon {
    width: 72rpx;
    height: 72rpx;
}

.ms-links-item-content {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    padding-left: 16rpx;
}

.ms-links-item-name {
    align-items: center;
    flex: 1;
    font-size: 28rpx;
    color: #333;
    display: flex;
    justify-content: center;
}

.ms-links-item-desc {
    align-items: center;
    flex: 1;
    font-size: 22rpx;
    color: #999;
    display: flex;
    justify-content: center;
}

.ms-nav,.userContent {
    width: 750rpx;
}

.ms-nav {
    box-sizing: border-box;
    padding: 20rpx 30rpx 0;
}

.ms-nav-item {
    box-sizing: border-box;
    display: flex;
    align-items: center;
    padding: 26rpx 0;
}

.ms-nav-item-divide {
    border-bottom: 1px solid #eee;
    padding-bottom: 40rpx;
}

.ms-ni-icon {
    width: 36rpx;
    height: 36rpx;
}

.ms-ni-main {
    display: flex;
    align-items: center;
    padding-left: 22rpx;
}

.ms-ni-name {
    font-size: 28rpx;
    color: #333;
}

.ms-ni-new {
    margin-left: 32rpx;
    width: 68rpx;
    height: 28rpx;
    line-height: 28rpx;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    background-color: #ff6b69;
}

.ms-ni-option {
    flex: auto;
    display: flex;
    justify-content: flex-end;
}

.ms-ni-option view {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background-color: #fb3633;
    margin-right: 24rpx;
}

.ms-ni-more {
    width: 14rpx;
    height: 24rpx;
}

.rightcontent {
    position: relative;
    width: 654rpx;
    height: 40px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.rightcontent text {
    font-size: 14px;
    letter-spacing: 1px;
    color: #333;
}

.rightcontent image {
    width: 18rpx;
    height: 28rpx;
}

.contactBox {
    width: 600rpx;
    height: 38px;
    line-height: 38px;
    text-align: start;
    color: #333;
    font-size: 14px;
    background: #fff;
    padding: 0;
    margin: 0;
}

.switchBox {
    position: relative;
    right: -12rpx;
}
.creatmyalbum {
    width: 482rpx;
    height: 66rpx;
    background: url('http://oibl5dyji.bkt.clouddn.com/creatmyalbum.png') no-repeat center;
    background-size: 482rpx 66rpx;
    margin-bottom: 52rpx;
}
.set_btn{
  width: 70%;
  text-align: center;
  margin: 15%;
}
.agree{
  background:#ff6b01;
  line-height: 80rpx;
  border-radius: 40rpx;
}