.pvModel,.funModel {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 9999;
}

.pvModel-box,.funModel-box {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: flex-end;
    animation: bgColor 0.5s ease;
    background: rgba(0,0,0,0.5);
}

.pvModel-main {
    width: 750rpx;
    background: #efeff5;
    animation: pvBottom 0.3s ease;
    position: relative;
}

@-webkit-keyframes pvBottom {
    0% {
        bottom: -310rpx;
    }

    100% {
        bottom: 0rpx;
    }
}

@keyframes pvBottom {
    0% {
        bottom: -310rpx;
    }

    100% {
        bottom: 0rpx;
    }
}

@-webkit-keyframes pvTop {
    0% {
        margin-top: 90rpx;
    }

    50% {
        margin-top: 135rpx;
    }

    100% {
        margin-top: 112rpx;
    }
}

@keyframes pvTop {
    0% {
        margin-top: 90rpx;
    }

    50% {
        margin-top: 135rpx;
    }

    100% {
        margin-top: 112rpx;
    }
}

@-webkit-keyframes pvOpacity {
    0% {
        opacity: 0;
    }

    70% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@keyframes pvOpacity {
    0% {
        opacity: 0;
    }

    70% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

