.navbar {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    background: #fff;
    border-top: 2rpx solid #f5f5f5;
    width: 100%;
}

.navbar .item {
    position: relative;
    -webkit-box-flex: 1;
    flex: 1;
    text-align: center;
    line-height: 80rpx;
    font-size: 30rpx;
}

.navbar .item.active {
    color: #E51C23;
}

.navbar .item.active:after {
    width: 120rpx;
    content: "";
    position: absolute;
    bottom: 0;
    left: 50%;
    margin-left: -60rpx;
    height: 4rpx;
    background: #E51C23;
}

.member-intro {
    padding: 34rpx 100rpx 20rpx;
    background: #fff;
    margin-top: 18rpx;
}

.touxiang {
    display: block;
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
}

.user-name {
    margin: 14rpx 0;
    font-size: 32rpx;
}

.level {
    font-size: 24rpx;
}

.team-top-rt {
    text-align: center;
}

.team-number {
    font-size: 72rpx;
    color: #E51C23;
    margin-top: 40rpx;
}

.jibie {
    width: 100%;
    background: #fff;
    line-height: 70rpx;
    padding-top: 20rpx;
    margin-bottom: 20rpx;
}

.jibie .active {
    color: #E51C23;
}

.different-identity {
    margin: 0 50rpx;
    font-size: 28rpx;
}

.agent {
    width: 100%;
    background: #fff;
}

.agent-item {
    padding: 20rpx 0;
    margin: 0 26rpx;
    border-bottom: 1px solid #f5f5f5;
    background: #fff;
}

.agent-item:last-child {
    border-bottom: none;
}

.agent-item-lt {
    position: relative;
    width: 100rpx;
    height: 100rpx;
}

.first-img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}

.second-img {
    width: 100rpx;
    height: 25rpx;
    position: absolute;
    bottom: 0;
    left: 0;
}

.agent-item-mid {
    -webkit-box-flex: 1;
    flex: 1;
    padding-left: 40rpx;
}

.agent-item-mid-view {
    margin-bottom: 20rpx;
}

.agent-item-mid-view image {
    width: 34rpx;
    height: 34rpx;
    margin-right: 10rpx;
    vertical-align: middle;
}

.agent-item-rt {
    width: 12rpx;
    height: 24rpx;
    margin-top: 16rpx;
}

.user-name-1 {
    margin-right: 10rpx;
    font-size: 32rpx;
}

.time {
    color: #999;
}

.days {
    color: #999;
}

.flexed {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: space-between;
    -ms-flex-pack: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.flexed-2 {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: flex-start;
    -ms-flex-pack: flex-start;
    justify-content: flex-start;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.flexed-3 {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.tps {
    color: #FF0000;
}

.team-assign {
    font-size: 27rpx;
    background: #ec850c;
    color: #fff;
    padding: 10rpx 20rpx;
    margin-top: 15rpx;
    border-radius: 10rpx;
}

.team-top {
    flex-wrap: wrap;
}

.team-top-lt {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
}

.user-name {
    width: 100%;
    display: flex;
    justify-content: center;
}

.team-top-rt {
    width: 100%;
}