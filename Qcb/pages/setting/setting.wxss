.list-con {
    width: 100%;
    background: #fff;
    height: 100%;
}

.h140 {
    height: 140rpx!important;
}

.list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 25rpx;
    border-bottom: 1rpx solid #f5f5f5;
    height: 100rpx;
}

.list text {
    font-size: 28rpx;
    color: #4c4c4c;
}

.list-rt {
    display: flex;
    align-items: center;
    font-size: 28rpx;
    width: 80%;
    height: 100%;
    justify-content: flex-end;
}

.sex {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.list-rt input {
    color: #999999;
    font-size: 24rpx;
    text-align: right;
    padding-right: 10rpx;
    width: 100%;
}

.list-rt .tx {
    width: 97rpx;
    height: 97rpx;
    border-radius: 50%;
}

.list-rt image {
    width: 12rpx;
    height: 21rpx;
    margin-right: 10rpx;
}

.but button {
    width: 704rpx;
    height: 85rpx;
    background-color: #ed6d00;
    color: #fff;
    font-size: 32rpx;
    margin-top: 70rpx;
}