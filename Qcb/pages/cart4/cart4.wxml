<view class="paysuccess-main">
    <image src="../../image/paysuccess-icon.png" style="width:180rpx; height:180rpx;"></image>
    <view class="paysuc-tit">提交成功</view>
    <view class="paysuc-order">
        <view>订单已经提交成功，请尽快完成支付</view>
    </view>
    <view class="paysuc-orderinfo">
        <view>订单号：{{order.order_sn}}</view>
        <view>订单金额：
      <label class="pay-red">￥{{order.order_amount}}</label>
        </view>
    </view>
    <view bindtap="PayOrder" class="pay-btn pay-back">立即支付</view>
    <view bindtap="GoOrder" class="pay-btn pay-order">查看订单</view>
</view>
