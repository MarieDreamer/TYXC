page {
    height: 100%;
}

.au-container {
    height: 100%;
}

.au-section {
    width: 750rpx;
    height: 100%;
    background: url("http://oibl5dyji.bkt.clouddn.com/activecenterloginbackground.png") no-repeat center;
    background-size: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.au-section-yinian {
    width: 310rpx;
    height: 310rpx;
    margin-top: 50rpx;
}

.au-section-center {
    margin-top: 50rpx;
    width: 750rpx;
    height: 180rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.au-section-center image {
    width: 585rpx;
    height: 50rpx;
}

.au-section-login {
    margin-top: 140rpx;
    width: 300rpx;
    height: 88rpx;
    background-color: #ffe500;
    border-radius: 44rpx;
    color: #333;
    font-size: 40rpx;
    font-weight: bold;
    display: flex;
    justify-content: center;
    align-items: center;
}