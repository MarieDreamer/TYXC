page {
    background: #f8f8f8;
}

.mydata {
    background: #ffee53;
    width: 100%;
    height: 148rpx;
    display: flex;
    position: relative;
}

.mydata view:nth-child(1) {
    display: flex;
    flex-direction: column;
    margin-left: 24rpx;
    margin-top: 12rpx;
}

.mydata view:nth-child(2) {
    display: flex;
    flex-direction: column;
    margin-left: 42rpx;
}

.mydata view:nth-child(3) {
    display: flex;
    position: absolute;
    right: 32rpx;
    top: 46rpx;
}

.mydata view:nth-child(1) image:nth-child(1) {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-top: 12rpx;
    margin: 0 auto;
}

.mydata view:nth-child(1) image:nth-child(2) {
    width: 104rpx;
    height: 34rpx;
    margin: 0 auto;
    margin-top: -10rpx;
}

.mydata view:nth-child(2) text:nth-child(1) {
    font-family: PingFang-SC-Medium;
    font-size: 48rpx;
    color: #333333;
    letter-spacing: 0;
    margin-top: 20rpx;
}

.mydata view:nth-child(2) text:nth-child(2) {
    font-family: PingFangSC-Regular;
    font-size: 11px;
    color: #666666;
    letter-spacing: 0;
    margin-top: 14rpx;
}

.mydata view:nth-child(3) text:nth-child(1) {
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    color: #666666;
    letter-spacing: 0;
    text-align: center;
    vertical-align: middle;
}

.mydata view:nth-child(3) image:nth-child(2) {
    width: 28rpx;
    height: 28rpx;
    margin-top: 10rpx;
    margin-left: 12rpx;
    vertical-align: middle;
}

.photolist {
    margin-top: 20rpx;
}

.photolist .template {
    display: flex;
}

.photolist .template .index image {
    width: 40rpx;
    height: 56rpx;
    vertical-align: middle;
    margin-left: 22rpx;
    margin-right: 32rpx;
}

.photolist .template .index {
    margin-top: 40rpx;
}

.photolist .template .index text {
    font-family: PingFang-SC-Medium;
    font-size: 32rpx;
    color: #999999;
    letter-spacing: 0;
    margin: 0rpx 44rpx 0 30rpx;
}

.num {
    width: 100rpx;
}

.photolist .template .desc {
    width: 100%;
    display: flex;
    flex-flow: row wrap;
    -ms-flex-pack: justify;
    justify-content: space-between;
    border-bottom: 1px solid #d8d8d8;
    position: relative;
}

.photolist .template .desc view:nth-child(1) {
    padding-top: 30rpx;
    padding-bottom: 26rpx;
}

.photolist .template .desc view:nth-child(1) image {
    width: 76rpx;
    height: 76rpx;
    border-radius: 50%;
    vertical-align: middle;
    margin-right: 20rpx;
}

.photolist .template .desc view:nth-child(1) .name {
    font-family: PingFangSC-Regular;
    font-size: 20px;
    color: #333333;
    letter-spacing: 0;
}

.photolist .template .desc view:nth-child(2) {
    position: absolute;
    right: 32rpx;
    top: 40rpx;
}

.photolist .template .desc view:nth-child(2) text {
    font-family: PingFangSC-Regular;
    font-size: 18px;
    color: #666666;
    letter-spacing: 0;
}

.photolist .template .desc view:nth-child(2) image {
    width: 28rpx;
    height: 28rpx;
    margin-left: 12rpx;
}

.red {
    color: #ff6161;
}

.desc wx-view wx-text:nth-child(2), {
    display: inline-block;
    width: 300rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: middle;
}

.myname {
    display: inline-block;
    width: 300rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: middle;
}