}

.oneline,.twoline,.threeline {
    position: absolute;
    left: 50%;
    top: 50%;
    z-index: 200;
    transform: translate(-50%,-50%);
    background: transparent;
}

.oneline {
    width: 296rpx;
    height: 296rpx;
    border-radius: 100%;
    border: 1px solid rgba(53,53,53,0.9);
}

.twoline {
    width: 344rpx;
    height: 344rpx;
    border-radius: 100%;
    border: 1px solid rgba(53,53,53,0.5);
}

.threeline {
    width: 392rpx;
    height: 392rpx;
    border-radius: 100%;
    border: 1px solid rgba(53,53,53,0.3);
}

.showlongtapcover {
    position: absolute;
    top: 0;
    z-index: 1000;
    animation: opt 5s infinite;
}

.showlongtapcover text {
    position: absolute;
    display: inline-block;
    bottom: 36rpx;
    left: 50%;
    transform: translateX(-50%);
    color: white;
    font-size: 30rpx;
    text-shadow: 2px 2px 5px #888;
}

.showlongtapcover text::before,.showlongtapcover text::after {
    content: '';
    display: block;
    position: absolute;
    width: 160rpx;
    height: 2rpx;
    background: white;
    transform: scaleY(0.5);
    top: 50%;
    box-shadow: 2px 2px 5px #888;
}

.showlongtapcover text::before {
    left: -180rpx;
    transform: translateY(50%);
}

.showlongtapcover text::after {
    right: -180rpx;
    transform: translateY(-50%);
}

