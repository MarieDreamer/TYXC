                    <form bindsubmit="toEditPhotosDelete" class="funMainItem" data-photosource="photosync" reportSubmit="true" wx:if="{{eventlist[selNumA].list[0].eMain==0||eventlist[selNumA].list[0].eMain==6}}">
                        <button class="funMainItem_btn" formType="submit">
                            <text>同步到其他相册</text>
                            <image src="http://oibl5dyji.bkt.clouddn.com/0506_evetnItem_right.png"></image>
