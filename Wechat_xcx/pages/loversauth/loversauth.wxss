page {
    height: 100%;
}

.loveAu-container {
    height: 100%;
}

.loveAu-section {
    position: relative;
    width: 750rpx;
    height: 100%;
    display: flex;
}

.loveAu-section-btn {
    padding: 0;
    margin: 0;
    border-radius: 0;
    width: 100%;
    height: 100%;
}

.loveAu-section image {
    width: 100%;
    height: 100%;
}

.loveAu-section .closeAuth {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 66rpx;
    height: 66rpx;
}