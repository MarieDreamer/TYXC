    90% {
        background: rgba(0,0,0,0.6);
    }

    100% {
        background: rgba(0,0,0,0);
    }
}

@-webkit-keyframes alading {
    0% {
        width: 0;
        height: 0;
    }

    20% {
        width: 289rpx;
        height: 227rpx;
    }

    24% {
        width: 255rpx;
        height: 200rpx;
    }

    28% {
        width: 279rpx;
        height: 218rpx;
    }

    32% {
        width: 268rpx;
        height: 210rpx;
    }

    90% {
        width: 268rpx;
        height: 210rpx;
    }

    100% {
        width: 0;
        height: 0;
    }
}

@keyframes alading {
    0% {
        width: 0;
        height: 0;
    }

    20% {
        width: 289rpx;
        height: 227rpx;
    }

    24% {
